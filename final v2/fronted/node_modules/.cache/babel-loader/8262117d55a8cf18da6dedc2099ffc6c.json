        <div class=\"col-lg-12\">\r\n                            <div class=\"inner text-center\">\r\n                                <h1 class=\"subtitle theme-gradient\" data-aos=\"fade-up\" data-aos-delay=\"150\">\r\n                                    色彩分析\r\n                                </h1>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <!-- End Slider Area -->\r\n\r\n        <!-- Start Counterup Area -->\r\n        <div class=\"our-counterup-area\">\r\n  <div class=\"container\">\r\n    <div class=\"row\">\r\n      <div class=\"col-lg-12\">\r\n        <div class=\"thumbnail\" data-aos=\"fade-up\">\r\n            <img class=\"radius w-100\" :src=\"getPicFromSection(this.$route.params.Typeid,this.$route.params.id)\" alt=\"/\"/>\r\n        </div>\r\n        \r\n      </div>\r\n    </div>\r\n  </div>\r\n</div>\r\n\r\n        <!-- End Counterup Area -->\r\n\r\n        <div class=\"color-blocks\" >\r\n    <div class=\"color-block\" v-for=\"color in colors\" :style=\"{ backgroundColor: color }\"></div>\r\n  </div>\r\n        <Separator/>\r\n\r\n        \r\n\r\n    </Layout>\r\n</template>\r\n\r\n<script>\r\n    import Layout from \"../../components/common/Layout\";\r\n    import Separator from \"../../components/elements/separator/Separator\";\r\n    import SectionTitle from \"../../components/elements/sectionTitle/SectionTitle\";\r\n    import PortfolioItemMixinFrame from '../../mixins/PortfolioItemMixinFrame'\r\n    import axios from 'axios'\r\n\r\n    export default {\r\n        name: 'WebAgency',\r\n        components: {  SectionTitle, Separator, Layout},\r\n        mixins: [PortfolioItemMixinFrame], \r\n        mounted(){\r\n           //console.log(this.$route.params.Typeid)\r\n           //console.log(this.$route.params.id)\r\n           this.fetchRGBValues();\r\n           //console.log(this.colors)\r\n        },\r\n        data() {\r\n            \r\n            return {\r\n                selectedSection:null,\r\n                section:{},\r\n                pic:null,\r\n                colors: [],\r\n                rgbValues: []\r\n            }\r\n        },\r\n        methods:{\r\n            getPicFromSection(section, id) {\r\n      const selectedSection = this.portfolioItems.find(item => item.section === section);\r\n      //console.log(selectedSection)\r\n      if (selectedSection) {\r\n        const selectedItem = selectedSection.items.find(item => item.id == id);\r\n        //console.log(selectedItem)\r\n        if (selectedItem) {\r\n          return selectedItem.portfolioImage;\r\n        }\r\n      }\r\n      return [];\r\n    },\r\n    fetchRGBValues() {\r\n  const Typeid = this.$route.params.Typeid;\r\n  const id = this.$route.params.id;\r\n  const url = `/${Typeid}output.txt`;\r\n\r\n  axios\r\n    .get(url)\r\n    .then(response => {\r\n      const data = response.data;\r\n      const regex = new RegExp(`image${id}\\\\.jpg:\\\\s*\\\\[([^\\\\]]+)\\\\]`);\r\n      const match = data.match(regex);\r\n\r\n      if (match) {\r\n        const rgbString = match[1];\r\n        console.log('rgbString:', rgbString);\r\n        const rgbValues = rgbString.match(/\\d+/g);\r\n        console.log('rgbValues:', rgbValues);\r\n        const rgbArray = [];\r\n\r\n        for (let i = 0; i < rgbValues.length; i += 3) {\r\n          const r = parseInt(rgbValues[i]);\r\n          const g = parseInt(rgbValues[i + 1]);\r\n          const b = parseInt(rgbValues[i + 2]);\r\n\r\n          if (!isNaN(r) && !isNaN(g) && !isNaN(b)) {\r\n            const rgbString = `rgb(${r}, ${g}, ${b})`;\r\n            rgbArray.push(rgbString);\r\n          } else {\r\n            console.error('RGB值格式不正确:', `${r}, ${g}, ${b}`);\r\n          }\r\n        }\r\n\r\n        console.log('rgbArray:', rgbArray);\r\n        this.colors = rgbArray;\r\n      } else {\r\n        console.error('无法找到对应的RGB值');\r\n      }\r\n    })\r\n    .catch(error => {\r\n      console.error('获取文档内容失败', error);\r\n    });\r\n}\r\n\r\n\r\n        }\r\n    }\r\n</script>\r\n<style>\r\n\r\n\r\n.color-blocks {\r\n  display: flex;\r\n  justify-content: center; /* 在主轴上居中对齐 */\r\n  margin-top: 20px; /* 色块与上方内容的间距 */\r\n  margin-bottom: 20px; /* 色块与上方内容的间距 */\r\n}\r\n\r\n.color-block {\r\n  width: 150px;\r\n  height: 80px;\r\n  margin-right: 10px; /* 色块之间的间距 */\r\n  /* 添加适当的样式，如间距、边框等 */\r\n}\r\n</style>"],"mappings":";AA+CA,OAAAA,MAAA;AACA,OAAAC,SAAA;AACA,OAAAC,YAAA;AACA,OAAAC,uBAAA;AACA,OAAAC,KAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IAAAJ,YAAA;IAAAD,SAAA;IAAAD;EAAA;EACAO,MAAA,GAAAJ,uBAAA;EACAK,QAAA;IACA;IACA;IACA,KAAAC,cAAA;IACA;EACA;;EACAC,KAAA;IAEA;MACAC,eAAA;MACAC,OAAA;MACAC,GAAA;MACAC,MAAA;MACAC,SAAA;IACA;EACA;EACAC,OAAA;IACAC,kBAAAL,OAAA,EAAAM,EAAA;MACA,MAAAP,eAAA,QAAAQ,cAAA,CAAAC,IAAA,CAAAC,IAAA,IAAAA,IAAA,CAAAT,OAAA,KAAAA,OAAA;MACA;MACA,IAAAD,eAAA;QACA,MAAAW,YAAA,GAAAX,eAAA,CAAAY,KAAA,CAAAH,IAAA,CAAAC,IAAA,IAAAA,IAAA,CAAAH,EAAA,IAAAA,EAAA;QACA;QACA,IAAAI,YAAA;UACA,OAAAA,YAAA,CAAAE,cAAA;QACA;MACA;MACA;IACA;IACAf,eAAA;MACA,MAAAgB,MAAA,QAAAC,MAAA,CAAAC,MAAA,CAAAF,MAAA;MACA,MAAAP,EAAA,QAAAQ,MAAA,CAAAC,MAAA,CAAAT,EAAA;MACA,MAAAU,GAAA,OAAAH,MAAA;MAEArB,KAAA,CACAyB,GAAA,CAAAD,GAAA,EACAE,IAAA,CAAAC,QAAA;QACA,MAAArB,IAAA,GAAAqB,QAAA,CAAArB,IAAA;QACA,MAAAsB,KAAA,OAAAC,MAAA,SAAAf,EAAA;QACA,MAAAgB,KAAA,GAAAxB,IAAA,CAAAwB,KAAA,CAAAF,KAAA;QAEA,IAAAE,KAAA;UACA,MAAAC,SAAA,GAAAD,KAAA;UACAE,OAAA,CAAAC,GAAA,eAAAF,SAAA;UACA,MAAApB,SAAA,GAAAoB,SAAA,CAAAD,KAAA;UACAE,OAAA,CAAAC,GAAA,eAAAtB,SAAA;UACA,MAAAuB,QAAA;UAEA,SAAAC,CAAA,MAAAA,CAAA,GAAAxB,SAAA,CAAAyB,MAAA,EAAAD,CAAA;YACA,MAAAE,CAAA,GAAAC,QAAA,CAAA3B,SAAA,CAAAwB,CAAA;YACA,MAAAI,CAAA,GAAAD,QAAA,CAAA3B,SAAA,CAAAwB,CAAA;YACA,MAAAK,CAAA,GAAAF,QAAA,CAAA3B,SAAA,CAAAwB,CAAA;YAEA,KAAAM,KAAA,CAAAJ,CAAA,MAAAI,KAAA,CAAAF,CAAA,MAAAE,KAAA,CAAAD,CAAA;cACA,MAAAT,SAAA,UAAAM,CAAA,KAAAE,CAAA,KAAAC,CAAA;cACAN,QAAA,CAAAQ,IAAA,CAAAX,SAAA;YACA;cACAC,OAAA,CAAAW,KAAA,kBAAAN,CAAA,KAAAE,CAAA,KAA